<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- pressed -->
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/swedenLightBlue" />
            <size android:height="65dp" android:width="65dp" />
        </shape>
    </item>
    
    <!-- default -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/swedenBlue" />
            <size android:height="65dp" android:width="65dp" />
        </shape>
    </item>
    
    
</selector>